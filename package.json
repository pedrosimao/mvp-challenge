{
  "name": "react-startup-starter",
  "version": "0.0.0",
  "author": {
    "name": "Pedro Sim√£o",
    "url": "https://github.com/PedroSimao"
  },
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:staging": "tsc && vite build --mode staging",
    "serve": "vite preview",
    "lint": "eslint src --ext .tsx --ext .ts",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --config ./.prettierrc -w 'src/**/*.{tsx,ts}' && git update-index --again",
    "pretest": "yarn lint:fix",
    "test": "vitest --passWithNoTests",
    "coverage": "vitest run --coverage",
    "test:typescript": "tsc",
    "test:watch": "yarn test --collectCoverage=false --watch",
    "cypress:open": "cypress open --browser chrome",
    "cypress:run": "cypress run --browser chrome",
    "gen:theme-typings": "chakra-cli tokens ./src/theme",
    "posttest": "npx http-server coverage/lcov-report",
    "prepare": "husky install",
    "postinstall": "yarn gen:theme-typings"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.5",
    "@chakra-ui/system": "^1.11.1",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@fontsource/roboto": "^4.5.3",
    "@vitejs/plugin-react": "^1.2.0",
    "axios": "^0.26.0",
    "babel-plugin-macros": "^3.1.0",
    "date-fns": "^2.28.0",
    "framer-motion": "^6.2.8",
    "react": "^17.0.2",
    "react-datepicker": "^4.7.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-query": "^3.34.16",
    "vite-plugin-svgr": "^1.0.1"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.9.0",
    "@cypress/react": "^5.12.4",
    "@cypress/vite-dev-server": "^2.2.2",
    "@emotion/babel-plugin": "^11.7.2",
    "@emotion/eslint-plugin": "^11.7.0",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@types/react": "^17.0.39",
    "@types/react-datepicker": "^4.3.4",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "c8": "^7.11.0",
    "cypress": "^9.5.1",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.4.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "happy-dom": "^2.43.1",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "rollup": "^2.68.0",
    "typescript": "~4.5.5",
    "vite": "^2.8.6",
    "vite-plugin-babel-macros": "^1.0.5",
    "vitest": "^0.5.9"
  }
}
